{
    "name": "goldfinch/dashcore",
    "description": "",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Artem Karasev"
    }],
    "require": {
        "php": ">=8.0",
        "silverstripe/framework": "^5.0",
        "silverstripe/admin": "^2.0",
        "slowprog/composer-copy-file": "~0.3",
        "twbs/bootstrap-icons": "^1"
    },
    "scripts": {
        "post-install-cmd": [
            "SlowProg\\CopyFile\\ScriptHandler::copy"
        ],
        "post-update-cmd": [
            "SlowProg\\CopyFile\\ScriptHandler::copy"
        ]
    },
    "autoload": {
        "psr-4": {
            "Goldfinch\\Dashcore\\": "src/"
        }
    },
    "extra": {
        "copy-file": {
            "vendor/twbs/bootstrap-icons/icons/": "clien/dist/bootstrap-icons/"
        },
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
